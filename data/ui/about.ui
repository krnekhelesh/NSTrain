<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.0 -->
  <object class="GtkAboutDialog" id="aboutdialog">
    <property name="can_focus">False</property>
    <property name="border_width">5</property>
    <property name="title" translatable="yes">NSTrain - About</property>
    <property name="resizable">False</property>
    <property name="modal">True</property>
    <property name="window_position">center</property>
    <property name="destroy_with_parent">True</property>
    <property name="icon">../media/app-icon.png</property>
    <property name="type_hint">dialog</property>
    <property name="program_name">NSTrain</property>
    <property name="version">0.7.1</property>
    <property name="comments" translatable="yes">Netherlands Train Scheduler
Not officially affliated with NS Railways</property>
    <property name="website">http://krnekhelesh.github.com/NSTrain/</property>
    <property name="website_label" translatable="yes">Homepage</property>
    <property name="authors">Nekhelesh Ramananthan</property>
    <property name="documenters">Nekhelesh Ramananthan</property>
    <property name="artists">Georgi Karavasilev</property>
    <property name="logo">../media/app-icon.png</property>
    <property name="license_type">gpl-3-0</property>
    <child internal-child="vbox">
      <object class="GtkBox" id="aboutdialog-vbox1">
        <property name="can_focus">False</property>
        <property name="orientation">vertical</property>
        <property name="spacing">2</property>
        <child internal-child="action_area">
          <object class="GtkButtonBox" id="aboutdialog-action_area1">
            <property name="can_focus">False</property>
            <property name="layout_style">end</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
